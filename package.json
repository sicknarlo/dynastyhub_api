{
  "name": "dynastyhub_api",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon",
    "tsc:w": "tsc -w",
    "lint": "tslint server/**/*.ts --fix",
    "test": "mocha",
    "build": "tsc -p ./prod.tsconfig.json && ncp './server' './dist'",
    "test:coverage": "nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "server/**/*.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "exclude": [
      "server/**/*.d.ts",
      "server/tests/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "check-coverage": "--lines 50 --per-file",
    "all": false,
    "sourceMap": false,
    "instrument": true
  },
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.1",
    "@types/mongoose": "^5.0.15",
    "@types/node": "^10.3.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "cron": "^1.3.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "fuzzyset.js": "^0.0.5",
    "mocha": "^5.2.0",
    "mongoose": "^5.1.4",
    "ncp": "^2.0.0",
    "node-cache": "^4.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^12.0.2",
    "redis": "^2.8.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "xml2js": "^0.4.19"
  }
}
